library(readr)

library(broom)

library(dplyr)

# Read data

df <- read_csv(

  "C:/Users/User/Downloads/team data/team 156/complete_project/members/member_A_data/cleaned_data.csv",

  show_col_types = FALSE

)

# Correlation tests

pearson <- cor.test(

  df$apartment_living_area_sqm,

  df$price_in_USD,

  method = "pearson"

spearman <- cor.test(

  df$apartment_living_area_sqm,

  df$price_in_USD,

  method = "spearman",

  exact = FALSE   # <- removes the "ties" warning

)
